<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="screen.main"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />
   
   	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
	 <uses-permission android:name="android.permission.CAMERA" />
	 <uses-feature android:name="android.hardware.camera" />
	 <uses-feature android:name="android.hardware.camera.autofocus" />
	
    <application android:icon="@drawable/icon" 
    			android:label="@string/app_name">
    			
        <activity android:name=".GoGatorActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="screen.level1.MapActivity"/>
       <uses-library android:name="com.google.android.maps" />
       
        <activity android:name="screen.level1.HomeActivity"></activity>
      	<activity android:name="screen.level1.CameraActivity"></activity>
        <activity android:name="screen.level1.MoreActivity"></activity>
        
		<activity android:name="screen.level2.MyCafeActivity"></activity>
		<activity android:name="screen.level2.MyCampusActivity"></activity>
		<activity android:name="screen.level2.MyCampusBuildingActivity"></activity>
		<activity android:name="screen.level2.MyCampusDeptActivity"></activity>
		<activity android:name="screen.level2.MyRideActivity"></activity>
		<activity android:name="screen.level2.MyVisitActivity"></activity>
		
		<activity android:name="screen.level3.MyCampusDetailActivity"></activity>
      </application>
      
</manifest>